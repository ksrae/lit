{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "lib": ["es2022", "dom"],
    "experimentalDecorators": true,
    "sourceMap": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "useDefineForClassFields": true, // <-- 이놈이 범인임.
    "types": ["vite/client"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}